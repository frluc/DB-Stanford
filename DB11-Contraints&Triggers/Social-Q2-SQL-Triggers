CREATE TRIGGER Q2A
AFTER INSERT ON Highschooler
FOR EACH ROW
WHEN New.grade < 9 OR New.grade > 12
BEGIN
    UPDATE Highschooler SET grade = NULL WHERE ID = New.ID;
END;

|

CREATE TRIGGER Q2B
AFTER INSERT ON Highschooler
FOR EACH ROW
WHEN New.grade IS NULL
BEGIN
    UPDATE Highschooler SET grade = 9 WHERE ID = New.ID;
END;
